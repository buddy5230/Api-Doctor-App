{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@now/node"
    }
  ],
  "routes": [
    
      { "src": "/database", "dest": "/db.js" },
      { "src": "/models", "dest": "/user.js" },
      { "src": "/route", "dest": "/user.route.js" },
      { "src": "/(.*)", "dest": "/server.js" }  
    
   ]
}
